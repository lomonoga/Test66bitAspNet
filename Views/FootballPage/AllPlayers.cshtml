@model System.Collections.Generic.IEnumerable<Test66bit.BLL.DTO.PlayerDTO>

@{
    ViewBag.Title = "AllPlayers";
    Layout = null;
}

<body>
<h2>All Players</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Forename)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Surname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Sex)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Country)
        </th>
        <th>
            TeamName
        </th>
    </tr>
    @foreach (var player in Model!)
    {
        <tr>
            <td>@player.Forename</td>
            <td>@player.Surname</td>
            <td>@player.Sex</td>
            <td>@player.Country</td>
            <td>@player.NewTeamName</td>
        </tr>
    }
</table>
</body>
